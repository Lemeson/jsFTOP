{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "view/**",
      "use": "@vercel/static"
    },
    {
      "src": "model/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/server.js"
    },
    {
      "src": "/model/(.*)",
      "dest": "model/$1"
    },
    {
      "src": "/(.*)",
      "dest": "view/$1"
    }
  ]
}